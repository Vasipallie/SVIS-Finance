{
  "expo": {
    "scheme": "SVISFinance",
    "name": "SVIS-Finance",
    "slug": "svis-finance",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SVIS-app.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SVIS-app.png",
      "resizeMode": "contain",
      "backgroundColor": "#160c28"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.SVIS.SVISFinance",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SVIS-app.png",
        "backgroundColor": "#160c28"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.SVIS.SVISFinance"
    },
    "web": {
      "favicon": "./assets/SVIS-app.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d4c9b9b8-61cb-4f26-b72c-55aa47f937a9"
      }
    },
    "owner": "svis-finance"
  }
}
